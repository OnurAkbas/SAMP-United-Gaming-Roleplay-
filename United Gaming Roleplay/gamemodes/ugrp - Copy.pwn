	{
		dest[ i ] = source[ i ];
	}
	dest[ count ] = 0;
}


public OnPlayerRename(name[],playerid)
{

	if (gPlayerLogged[playerid] != 0)
	{
		new query[ 1024 ], tmpname[ 32 ];
		mysql_real_escape_string( name, tmpname );
		format( query, sizeof( query ), "UPDATE `players` SET `name`='%s' WHERE`id`='%d'", tmpname, PlayerInfo[ playerid ][ pSQLID ] );
		mysql_query( query );
		SetPlayerName(playerid, tmpname);
		if(PlayerInfo[playerid][pPhousekey] != 0) //owns a house
		{
            new house = PlayerInfo[playerid][pPhousekey];
            strmid(HouseInfo[house][hOwner], GetPlayerNameEx(playerid), 0, strlen(GetPlayerNameEx(playerid)), 255);
            SaveHouses(house);
            OnPropTextUpdate(1,house);
		}
		if(PlayerInfo[playerid][pPbiskey] != 0) //owns a house
		{
            new bizz = PlayerInfo[playerid][pPbiskey];
            strmid(BizzInfo[bizz][bOwner], name, 0, strlen(name), 255);
            SaveBizz(bizz);
            OnPropTextUpdate(2,bizz);
		}
		if(PlayerInfo[playerid][PlayerCarModel2] > 0)
		{
		    strmid(CarInfo[PlayerInfo[playerid][PlayerCarModel2]][Owner], GetPlayerNameEx(playerid), 0, strlen(GetPlayerNameEx(playerid)), 255);
		}
		if(PlayerInfo[playerid][PlayerCarModel3] > 0)
		{
		    strmid(CarInfo[PlayerInfo[playerid][PlayerCarModel3]][Owner], GetPlayerNameEx(playerid), 0, strlen(GetPlayerNameEx(playerid)), 255);
		}
		if(PlayerInfo[playerid][PlayerCarModel] > 0)
		{
		    strmid(CarInfo[PlayerInfo[playerid][PlayerCarModel]][Owner], GetPlayerNameEx(playerid), 0, strlen(GetPlayerNameEx(playerid)), 255);
		}
		OnPlayerSave(playerid); // save player (in case server has a restart and fucks up their name)
	}
	return 1;
}

