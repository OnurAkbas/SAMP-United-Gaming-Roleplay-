stock CreateRadar(Float:x,Float:y,Float:z,Float:Angle,mph)
{
    for(new i = 0; i < sizeof(RadarInfo); i++)
  	{
  	    if(RadarInfo[i][SpeedCreated] == 0)
  	    {
            RadarInfo[i][SpeedCreated]=1;
            RadarInfo[i][SpeedX]=x;
            RadarInfo[i][SpeedY]=y;
            RadarInfo[i][SpeedZ]=z;
            RadarInfo[i][Speedmph] = mph;
            RadarInfo[i][SpeedObject] = CreateObject(18880,x+0.5,y,z-1.2,0,0,Angle);
	        return 1;
  	    }
  	}
  	return 0;
}

