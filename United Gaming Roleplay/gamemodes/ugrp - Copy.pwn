public BanAdd(playerid,reason[],admin[])
{
	new string[255], query[255], IP[16];
    new hour, minute, second, year, month, day;
	getdate(year, month, day);
	gettime(hour, minute, second);
	GetPlayerIp(playerid, IP, sizeof(IP));
	format(query, sizeof(query), "INSERT INTO `bans` (`name`, `reason`, `admin`, `ip`, `time`) VALUES('%s', '%s', '%s', '%s', '%d/%d/%d %d:%d:%d')",GetPlayer(playerid), reason, admin, IP, month, day, year, hour, minute, second);
	mysql_query(query);
	SendClientMessage(playerid, COLOR_LIGHTRED, "____________________________________________________________" );
	format(string, sizeof(string), "* You got banned by: {006600}%s, {FFFFFF}reason: {006600}%s", admin, reason);
	SendClientMessage(playerid,COLOR_WHITE, string);
	format(string, sizeof(string), "* Date: %d/%d/%d  Time: %d:%d:%d", day, month, year, hour, minute, second );
	SendClientMessage(playerid,COLOR_WHITE, string);
	SendClientMessage(playerid, COLOR_WHITE, "* If you feel you have been wrongfully banned post an appeal" );
	SendClientMessage(playerid, COLOR_WHITE, "* at www.ug-rp.com" );
	SendClientMessage(playerid, COLOR_LIGHTRED,"____________________________________________________________" );
	format(string, sizeof(string), "Banned By: %s Reason: %s", admin, reason); //mod
	BanEx(playerid, string);
	return 1;
}

