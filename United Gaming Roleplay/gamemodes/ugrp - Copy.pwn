public SetVehicleEngine(vehicleid, playerid)
{
	new engine,lights,alarm,doors,bonnet,boot,objective;
    GetVehicleParamsEx(vehicleid,engine,lights,alarm,doors,bonnet,boot,objective);
    if(engine == VEHICLE_PARAMS_OFF || engine == VEHICLE_PARAMS_UNSET)
	{
		new Float: f_vHealth , string[254];
		GetVehicleHealth(vehicleid, f_vHealth);
		if(f_vHealth < 350.0)
		{
		SendClientMessage(playerid, COLOR_LIGHTRED, "   The car won't start - it's totalled!, /service mechanic");
		format(string, sizeof(string), "* %s fails to start the engine.", GetPlayerNameEx(playerid));
		ProxDetector(30.0, playerid, string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
		return 1;
		}
		if(Gas[vehicleid] < 1) return SendClientMessage(playerid, COLOR_LIGHTRED, "   The car won't start - there's no fuel in the tank!, /service mechanic");
		SetVehicleParamsEx(vehicleid,VEHICLE_PARAMS_ON,lights,alarm,doors,bonnet,boot,objective);
		SendClientMessage(playerid, COLOR_WHITE, "Vehicle engine started successfully (/car engine to turn the engine off).");
	}
	return 1;
}

