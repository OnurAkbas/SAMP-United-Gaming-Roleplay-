	{
        if(gPlayerLogged[i] == 1)
	    {
  			if(PlayerInfo[i][pContractTime] > 0)
			{
				PlayerInfo[i][pContractTime] --;
			}
			if(PlayerInfo[i][pVip] > 0)
		    {
				if(PlayerInfo[i][pVipTime] > 0)
				{
					PlayerInfo[i][pVipTime]--;
				}
				if(PlayerInfo[i][pVipTime] <= 0)
				{
				    PlayerInfo[i][pVipTime] = 0;
				    PlayerInfo[i][pVip] = 0;
				    SendClientMessage(i, COLOR_SERVER, "**   Your VIP has expired, if you wish to restore it, contact an Executive Admin.");
