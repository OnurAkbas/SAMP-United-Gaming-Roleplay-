stock SendClientMessageF( playerid, color, message[], {Float,_}:... )
{
	new argcount = numargs( );
	if( argcount == 3 )
	{
		SendClientMessage( playerid, color, message );
		return;
	}
	for( new i = 0; i < argcount - 3; i++ )
	{
		for( new j = 0; j < 64; j++ )
		{
			args[ i ][ j ] = getarg( i + 3, j );
			if( !args[ i ][ j ] ) break;
		}
	}
	format( plmsg, sizeof( plmsg ), message, args[ 0 ], args[ 1 ], args[ 2 ], args[ 3 ], args[ 4 ],
		args[ 5 ], args[ 6 ], args[ 7 ], args[ 8 ], args[ 9 ] );
	SendClientMessage( playerid, color, plmsg );
}

