    {
        if(WeedInfo[w][WeedCreated] > 0)
        {
              WeedInfo[w][WeedTimer]++;
  	          if(WeedInfo[w][WeedTimer] <= 10)
  	          {
                WeedInfo[w][WeedGrams]++;
                WeedInfo[w][WeedZ] += 0.1;
                SetObjectPos(WeedInfo[w][WeedObject], WeedInfo[w][WeedX], WeedInfo[w][WeedY], WeedInfo[w][WeedZ]);
              }
 			  if(WeedInfo[w][WeedTimer] >= 20)
			  {
	            WeedInfo[w][WeedCreated]=0;
	            WeedInfo[w][WeedGrams]=0;
	            WeedInfo[w][WeedTimer]=0;
	            WeedInfo[w][WeedX]=0.0;
	            WeedInfo[w][WeedY]=0.0;
	            WeedInfo[w][WeedZ]=0.0;
	            DestroyObject(WeedInfo[w][WeedObject]);
			  }
        }
    }
    for(new h; h<MAX_HOUSES; h++)
    {
        if(HouseInfo[h][hOwned] == 1)
 	    {
	   	    HouseInfo[h][hTime] --;
	   	    if(HouseInfo[h][hTime] <= 0)
	   	    {
				 HouseInfo[h][hLock] = 1;
			     HouseInfo[h][hOwned] = 0;
  		 		 format(string, sizeof(string), "system has Successful removed inactive House %d: %s", h, HouseInfo[h][hOwner]);
                 StatLog(string);
				 strmid(HouseInfo[h][hOwner], "The State", 0, strlen("The State"), 255);
				 OnPropTextUpdate(1,h);
				 SaveHouses(h);
	        }
  	    }
    }
    for(new b; b<MAX_BUSINESSES; b++)
    {
        if(BizzInfo[b][bOwned] == 1)
 	    {
	   	    BizzInfo[b][bTime] --;
	   	    if(BizzInfo[b][bTime] <= 0)
	   	    {
				 BizzInfo[b][bLocked] = 1;
			     BizzInfo[b][bOwned] = 0;
