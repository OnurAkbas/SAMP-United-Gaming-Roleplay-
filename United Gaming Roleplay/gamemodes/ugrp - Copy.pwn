public Fillup(playerid)
{
	   	if(IsPlayerConnected(playerid))
	   	{
		    new VID;
		    new FillUp;
		    VID = GetPlayerVehicleID(playerid);
		    FillUp = 100 - Gas[VID];
			if(Refueling[playerid] == 1)
		    {
     	        if(IsALeo(playerid) || PlayerInfo[playerid][pMember] == 4 || PlayerInfo[playerid][pVip] > 0)
            	{
	               SendClientMessage(playerid,COLOR_LIGHTBLUE,"Your vehicle has been filled up with 100 liters of fuel for free.");
                   Gas[VID] += FillUp;
	            }
            	else
	            {
				  if(PlayerInfo[playerid][pCash] >= FillUp+4)
				  {
					 Gas[VID] += FillUp;
					 FillUp = FillUp * 8;
					 PlayerInfo[playerid][pCash] -= FillUp;
				     SendClientMessageF(playerid,COLOR_LIGHTBLUE,"* You filled your Vehicle up for: $%d.",FillUp);
		             new pay = FillUp / 8 * 4;
					 if(PointInfo[0][PointTaken] != 255) //fossil fuel
					 {
					    FamilyInfo[PointInfo[0][PointTaken]][FamilyBank] += pay;
				 	 }
				 	 TaxValue += pay;
				     Refueling[playerid] = 0;
				   }
			   	   else
			   	   {
			   	    SendClientMessage(playerid,COLOR_GREY,"   You can't afford that !");
			   	   }
		   	     }
		   	     Refueling[playerid] = 0;
		 	}
