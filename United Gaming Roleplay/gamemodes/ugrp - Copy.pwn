public IsPlayerBanned( playerid, reason[], bannedby[] )
{
	new ip[16], playername[32], strsplit[ 5 ][ 128 ], row[ 512 ];
	GetPlayerIp( playerid, ip, sizeof( ip ) );
	GetPlayerName( playerid, playername, 32 );
	new ipsplit[ 4 ][ 3 ];
	new Ranges[ 3 ][ 16 ];
	new query[1024];
	split( ip, ipsplit, '.' );
	format( Ranges[ 0 ], 16, "%s.%s.%s.*", ipsplit[ 0 ], ipsplit[ 1 ], ipsplit[ 2 ] );
	format( Ranges[ 1 ], 16, "%s.%s.*.*", ipsplit[ 0 ], ipsplit[ 1 ] );
	format( Ranges[ 2 ], 16, "%s.*.*.*", ipsplit[ 0 ] );
	format( query, sizeof( query ), "SELECT `reason`,`admin` FROM `bans` WHERE (`ip` = '%s' OR `ip` = '%s' OR `ip` = '%s' OR `ip` = '%s')",ip, Ranges[ 0 ], Ranges[ 1 ], Ranges[ 2 ] );
	mysql_query( query );
	mysql_store_result( );
	if ( mysql_num_rows( ) != 0 )
	{
		mysql_fetch_row( row );
		split( row, strsplit, '|' );
		set( reason, strsplit[ 0 ] );
		set( bannedby, strsplit[ 1 ] );
		return 1;
	}
	format( query, sizeof( query ), "SELECT `reason`,`admin` FROM `bans` WHERE `name` = '%s'", playername );
	mysql_query( query );
	mysql_store_result( );
	if ( mysql_num_rows( ) != 0 )
	{
		mysql_fetch_row( row );
		split( row, strsplit, '|' );
		set( reason, strsplit[ 0 ] );
		set( bannedby, strsplit[ 1 ] );
		return 1;
	}
	return 0;
}

set( dest[], const source[] )
{
