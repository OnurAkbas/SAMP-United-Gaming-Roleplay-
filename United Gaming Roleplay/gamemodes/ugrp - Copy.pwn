public OnObjectMoved(objectid)
{
    new Float:x, Float:y, Float:z;
	for(new i; i < sizeof(Obj_FloorDoors); i ++)
	{
		if(objectid == Obj_FloorDoors[i][0])
		{
		    GetObjectPos(Obj_FloorDoors[i][0], x, y, z);

		    if(x < X_DOOR_L_OPENED - 0.5)   // Some floor doors have shut, move the elevator to next floor in queue:
		    {
				Elevator_MoveToFloor(ElevatorQueue[0]);
				RemoveFirstQueueFloor();
			}
		}
	}

	if(objectid == Obj_Elevator)   // The elevator reached the specified floor.
	{
	    KillTimer(ElevatorBoostTimer);  // Kills the timer, in case the elevator reached the floor before boost.

	    FloorRequestedBy[ElevatorFloor] = INVALID_PLAYER_ID;

	    Elevator_OpenDoors();
	    Floor_OpenDoors(ElevatorFloor);

	    GetObjectPos(Obj_Elevator, x, y, z);
	    Label_Elevator	= Create3DTextLabel("Press 'F' to use elevator", 0xFFFFDD, 1784.9822, -1302.0426, z - 0.9, 4.0, 0, 1);

	    ElevatorState 	= ELEVATOR_STATE_WAITING;
	    SetTimer("Elevator_TurnToIdle", ELEVATOR_WAIT_TIME, 0);
	}

	return 1;
}


public IsPlayerBlocked(playerid)
{
	if(PlayerTied[playerid] == 0 && PlayerCuffed[playerid] == 0 && PlayerInfo[playerid][pHospital] == 0 && Injured[playerid] == 0)
	{
	    return 0;
	}
	return 1;
}

public DropGun(playerid, gun, ammo)
{
	if(HaveWeapon(playerid,gun) == gun)
	{
	    new slot = MAX_DROPGUNS;
	    for(new r; r<MAX_DROPGUNS; r++)
	    {
	        if(GunObjectInfo[r][oID] == 0)
	        {
	            slot = r;
	            break;
			}
	    }
	    if(slot == MAX_DROPGUNS)
	    {
	        new highvar = DROPGUN_TIMER+1;
            for(new r; r<MAX_DROPGUNS; r++)
		    {
		        if(GunObjectInfo[r][oTimer] < highvar)
		        {
		            highvar = GunObjectInfo[r][oTimer];
		            slot = r;
				}
		    }
		    DestroyDynamicObject(GunObjectInfo[slot][oID]);
		    GunObjectInfo[slot][oID] = 0;
		}
	    TakeWeapon(playerid,gun);
	    GunObjectInfo[slot][oAmmo] = ammo;
	    GunObjectInfo[slot][oGun] = gun;
		GetPlayerPos(playerid, GunObjectInfo[slot][oPosition][0], GunObjectInfo[slot][oPosition][1], GunObjectInfo[slot][oPosition][2]);
		GetPlayerFacingAngle(playerid, GunObjectInfo[slot][oPosition][3]);
		GunObjectInfo[slot][oID] = CreateDynamicObject(GunObjects[gun], GunObjectInfo[slot][oPosition][0], GunObjectInfo[slot][oPosition][1], GunObjectInfo[slot][oPosition][2]-1, 93.7, 120.0, GunObjectInfo[slot][oPosition][3]);
		GunObjectInfo[slot][oTimer] = DROPGUN_TIMER;
		return 1;
	}
	return 0;
}

