public OnVehicleSpawn(vehicleid)
{
    for(new i = 0; i < sizeof(CarInfo); i++)
   	{
        if(CarInfo[i][CarLinkID] == vehicleid)
   	    {
           SetVehicleModifications(i);
  		   ChangeVehicleColor(CarInfo[i][CarLinkID], CarInfo[i][CarColor1],CarInfo[i][CarColor2]);
        }
  	}
    if(IsValidObject(Neon[vehicleid])) { DestroyObject(Neon[vehicleid]); Neon[vehicleid] = 0; }
	if(IsValidObject(Neon2[vehicleid])) { DestroyObject(Neon2[vehicleid]); Neon2[vehicleid] = 0; }
	if(IsValidObject(Neon3[vehicleid])) { DestroyObject(Neon3[vehicleid]); Neon3[vehicleid] = 0; }
	if(IsValidObject(Neon4[vehicleid])) { DestroyObject(Neon4[vehicleid]); Neon4[vehicleid] = 0; }
    if(gDestroyVehicle[vehicleid])  									// if vehicle id is marked for deletion
    {
        ////foreach(Player, i)                            					// for all players
        for(new i; i<MAX_PLAYERS; i++)
		{
			if(IsPlayerConnected(i) && IsPlayerInVehicle(i,vehicleid))      	// if connected and in vehicleid
			{
				RemovePlayerFromVehicle(i);                                     	// remove from vehicle
			}
		}
    	SetVehicleVirtualWorld(vehicleid,255);                                  // set vehicle to unused vw to prevent players from using it
    	SetTimerEx("KillCar",10000,0,"d",vehicleid);
		for(new i = 0; i < sizeof(CreatedCars); i++)
    	{
    	    if(CreatedCars[i] == vehicleid)
    	    {
    	        CreatedCars[i] = INVALID_VEHICLE_ID;
    	        break;
			}
    	}
    }
	return 1;
}

public OnVehicleStreamIn(vehicleid, forplayerid)
{
	for(new c = 0; c < sizeof(CarInfo); c++)
	{
	    if( CarInfo[c][CarLinkID] == vehicleid )
	    {
	        if(CarInfo[c][CarLock] == 1 )
	        {
	            for(new u = 0; u < MAX_PLAYERS; u++)
	            {
		            SetVehicleParamsForPlayer( vehicleid, u, 0, CarInfo[c][CarLock] );
				}
			}
	    }
	}
	return 1;
}

